<% content_for :page_title do %>
  <%= link_to plural_resource_name(Spree::HomepageBanner), spree.admin_homepage_banners_url %> /
  <%= @homepage_banner.title %>
<% end %>

<%= form_for [:admin, @homepage_banner] do |f| %>
  <fieldset>
    <%= render partial: 'food_ui/admin/homepage_banners/form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
